<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".ArduinoMain"
    android:id="@+id/layer">

    <Button
        android:id="@+id/L1N"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 1 on"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/L1F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 1 off"
        android:layout_alignTop="@+id/L1N"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <Button
        android:id="@+id/L2N"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 2 on"
        android:layout_below="@+id/L1N"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/L2F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 2 off"
        android:layout_alignTop="@+id/L2N"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/L3N"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 3 on"
        android:layout_below="@+id/L2N"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/L3F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 3 off"
        android:layout_alignTop="@+id/L3N"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <Button
        android:id="@+id/L4N"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 4 on"
        android:layout_below="@+id/L3N"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/L4F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 4 off"
        android:layout_alignTop="@+id/L4N"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <Button
        android:id="@+id/L5N"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 5 on"
        android:layout_below="@+id/L4N"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/L5F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 5 off"
        android:layout_alignTop="@+id/L5N"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <Button
        android:id="@+id/L6N"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 6 on"
        android:layout_below="@+id/L5N"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/L6F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 6 off"
        android:layout_alignTop="@+id/L6N"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <Button
        android:id="@+id/L7N"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 7 on"
        android:layout_below="@+id/L6N"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/L7F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 7 off"
        android:layout_alignTop="@+id/L7N"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <Button
        android:id="@+id/L8N"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 8 on"
        android:layout_below="@+id/L7N"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/L8F"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Switch 8 off"
        android:layout_alignTop="@+id/L8N"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>